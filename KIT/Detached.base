views:
  - type: table
    name: Inbox
    filters:
      and:
        - state.containsAny("inbox")
    order:
      - file.name
      - date
      - state
    columnSize:
      file.name: 147
  - type: table
    name: Orphans
    filters:
      and:
        - state.containsAny("orphan")
    order:
      - file.name
      - date
      - state
    columnSize:
      file.name: 281
