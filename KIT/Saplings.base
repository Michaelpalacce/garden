views:
  - type: table
    name: Saplings
    filters:
      and:
        - state.containsAny("sapling")
        - kind != "anime"
        - file.folder != "DnD"
        - file.folder != "Buttons"
        - file.folder != "Templates"
        - file.folder != "Assets"
        - kind != "book"
        - kind != "meeting"
        - kind != "task"
        - '!state.contains("withered")'
    limit: 25
